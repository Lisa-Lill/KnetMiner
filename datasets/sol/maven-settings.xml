<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          https://maven.apache.org/xsd/settings-1.0.0.xsd">
                            
	<profiles>
		<profile>
			<id>docker</id>
			<properties>
				<knetminer.dataSourceId>solknet</knetminer.dataSourceId>
				<knetminer.specieName>Solanaceae</knetminer.specieName>
				<knetminer.appDescription>Knetminer instance about domestic tomato (solanum lycopersium), pepper (Capsicum annuum) and potato (Solanum tuberosum)</knetminer.appDescription>
				<knetminer.appKeywords>tomato, pepper, potato, knetminer</knetminer.appKeywords>
				<knetminer.specieTaxId>4081,4072,4113</knetminer.specieTaxId>
		
				<!-- If you define this with this placeholder, the build scripts will get the list from basemap.xml -->
			<!--	<knetminer.chromosomeList>%%knetminer.chromosomeList%%</knetminer.chromosomeList> -->
				
				<!--  true if you are providing a reference genome -->
				<knetminer.isReferenceGenomeProvided>false</knetminer.isReferenceGenomeProvided>
		
				<!-- 
					The contents of client/release_notes.html take priority over this, so it doesn't really matter
					if you define this property to override the default. We do it here just to recall this behaviour.
				-->				
				<knetminer.releaseNotesHtml></knetminer.releaseNotesHtml>
						
			</properties>
		</profile>
	</profiles>
</settings>
